<%= link_to image_tag('home.png',:width => 30, :height => 30, :class=>"t_padding_5"), root_path %>
    <span><%= notice %></span>
    <span><%= alert %></span>
  <% if signed_in? %>
    <span>Logged in as <%= current_user.email %>.</span>
    <%= link_to "Sign out", destroy_user_session_path, method: :delete, class:"right" %>
    <% if current_user.role?(:administrator) #can? :update, @user %>
      <%= link_to "Admin", admin_root_path, class:"right" %>
    <% else %>
      <%= link_to "我有车", new_admin_map_path, class:"right" %>
    <% end %>
  <% else %>
    <% if params[:controller]!="devise/sessions" %>
      <%= link_to "Sign in", new_user_session_path, class:"right" %>
    <% end %>
  <% end %>